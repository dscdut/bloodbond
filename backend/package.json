{
  "name": "nestjs-boilerplate",
  "version": "0.0.1",
  "description": "A nestjs boilerplate with full of prebuild features",
  "author": "Nguyen Ich Hoa",
  "private": true,
  "license": "MIT",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "typeorm": "ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli.js",
    "typeorm:run-migration": "npm run typeorm migration:run -- -d src/database/config.ts",
    "typeorm:generate-migration": "npm run typeorm -- -d src/database/config.ts migration:generate src/database/migrations/%name%",
    "typeorm:create-migration": "npm run typeorm -- migration:create src/database/migrations/%name%",
    "typeorm:revert-migration": "npm run typeorm -- -d src/database/config.ts migration:revert",
    "seed:create": "hygen seeds create",
    "seed:run": "ts-node -r tsconfig-paths/register ./src/database/seeds/run-seed.ts",
    "module:generate": "nest g module modules/%name%",
    "controller:generate": "nest g controller modules/%name%",
    "service:generate": "nest g service modules/%name%",
    "gen-entity-local": "npx typeorm-model-generator -h localhost -d bloodbond -u postgres -x postgres -e postgres -o src/database/entities  --skipTables=migrations,spatial_ref_sys --noConfig --cf=none --namingStrategy=./src/config/namingStrategy"
  },
  "dependencies": {
    "@nestjs/common": "^9.2.0",
    "@nestjs/config": "^2.2.0",
    "@nestjs/core": "^9.2.0",
    "@nestjs/jwt": "^9.0.0",
    "@nestjs/microservices": "^9.2.0",
    "@nestjs/passport": "^9.0.3",
    "@nestjs/platform-express": "^9.4.3",
    "@nestjs/swagger": "^6.2.1",
    "@nestjs/testing": "^9.2.0",
    "@nestjs/typeorm": "^9.0.1",
    "@sentry/node": "^7.61.0",
    "@sentry/tracing": "^7.61.0",
    "@types/lodash": "^4.14.195",
    "@types/multer": "^1.4.7",
    "amqp-connection-manager": "^4.1.14",
    "amqplib": "^0.10.3",
    "argon2": "^0.31.1",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.13.2",
    "cloudinary": "^1.37.2",
    "google-auth-library": "^8.8.0",
    "hygen": "^6.2.11",
    "lodash": "^4.17.21",
    "logform": "^2.4.2",
    "multer": "^1.4.5-lts.1",
    "nestjs-i18n": "^10.3.7",
    "passport": "^0.6.0",
    "passport-google-oauth20": "^2.0.0",
    "passport-jwt": "^4.0.0",
    "passport-local": "^1.0.0",
    "pg": "^8.8.0",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "rxjs": "^7.5.7",
    "typeorm": "^0.3.11",
    "typeorm-model-generator": "^0.4.6",
    "winston": "^3.8.2",
    "winston-daily-rotate-file": "^4.7.1"
  },
  "devDependencies": {
    "@nestjs/cli": "^9.1.5",
    "@nestjs/schematics": "^9.0.3",
    "@types/express": "^4.17.14",
    "@types/jest": "^29.2.3",
    "@types/node": "^18.11.9",
    "@types/passport-google-oauth20": "^2.0.11",
    "@types/reflect-metadata": "^0.1.0",
    "@typescript-eslint/eslint-plugin": "^5.44.0",
    "@typescript-eslint/parser": "^5.44.0",
    "eslint": "^8.28.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.2.1",
    "prettier": "^2.8.0",
    "source-map-support": "^0.5.21",
    "ts-loader": "^9.4.1",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^4.9.3"
  }
}
